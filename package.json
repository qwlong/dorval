{
  "name": "dorval-workspace",
  "version": "0.2.4",
  "description": "Dart code generator for OpenAPI specifications",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "version:sync": "node scripts/sync-versions.js",
    "version:check": "yarn workspaces foreach exec echo $npm_package_name@$npm_package_version",
    "version:update": "node scripts/version.js",
    "prerelease": "yarn version:check",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "publish:npm": "./scripts/publish.sh",
    "ci": "yarn build && yarn test && yarn lint"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^20.13.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "prettier": "3.6.2",
    "semantic-release": "^24.2.8",
    "tsup": "^8.5.0",
    "turbo": "^1.13.3",
    "typescript": "^5.2.2",
    "vitest": "^0.6.3"
  },
  "engines": {
    "node": ">=20.8.1"
  }
}
